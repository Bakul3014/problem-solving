#include<bits/stdc++.h>
using namespace std;
int main(){
string b;
cin>>b;
if(b=="vertebrado"){
cin>>b;
if(b=="ave"){
    cin>>b;
    if(b=="carnivoro"){

        cout<<"aguia"<<endl;
    }else{
    cout<<"pomba"<<endl;
    }
}else{
cin>>b;

    if(b=="onivoro"){
        cout<<"homem"<<endl;
    }else{
    cout<<"vaca"<<endl;

    }



}

}else{
    cin>>b;
    if(b=="inseto"){

        cin>>b;
        if(b=="hematofago"){
            cout<<"pulga"<<endl;
        }else{
        cout<<"lagarta"<<endl;
        }
    }else{
    cin>>b;
    if(b=="hematofago"){

        cout<<"sanguessuga"<<endl;
    }else{
    cout<<"minhoca"<<endl;
    }
    }


}


return 0;
}
